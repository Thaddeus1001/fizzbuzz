	private static double[] convolution (double[] f, double[] g){
		double N = f.length;
		double M = g.length;
		for (int i = 0; f.length < (N-M+1); i++){
			for (int j = 0; g.length < M; j++){
				double[] convolution = ((M-1)-f[i])*g[j];
					return convolution[];
			}
		}
			
			
	}
